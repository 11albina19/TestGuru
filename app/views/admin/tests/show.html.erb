<p>
  <strong>Название:</strong>
  <h2><%= @test.title %></h2>
</p>
<p>
  <strong>Уровень:</strong>
  <%= @test.level %>
</p>
<table>
  <tr>
    <th>Тест содержит вопросы</th>
    <th colspan="3">Действия</th>
  </tr>
  <%= render @questions %>
  <%= render partial: 'admins/questions/question', collection: @test.questions %>
</table>
<%= link_to 'Назад', admin_tests_path %>
<%= link_to 'Редактировать тест', edit_admin_test_path(@test) %>
<%= link_to 'Добавить вопрос', new_admin_test_question_path(@test) %>
