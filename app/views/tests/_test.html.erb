<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= amount_questions(test) %></td>
  <td><%= link_to 'Открыть', test_path(test) %></td>
  <td><%= link_to 'Редактировать', edit_test_path(test) %></td>
  <td>
    <%= link_to 'Удалить',
                test_path(test),
                data: { turbo_method: :delete, turbo_confirm: "Вы уверены?" }%>
  </td>
  <td><%= button_to 'Старт', start_test_path(test) %></td>
</tr>
